FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.00754;

boundaryField
{
    inlet
    {
        type            cyclic;
    }
    outlet
    {
        type            cyclic;
    }
    fixedWalls
    {
        type            epsilonWallFunction;
        value           uniform 0.026;
    }
    
    rigidLid
    {
    	type 		zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}


