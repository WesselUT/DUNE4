    {
        type            cyclic;
    }
    outlet
    {
        type            cyclic;
    }	
    bed
    {
    	type 		nutkRoughWallFunction;
    	Ks		uniform 0.01;

//    	Ks		uniform 0.0035;
  	Cs		uniform 0.5;
//        type            nutkWallFunction;
        value           uniform 0;
